{
    "name": "imarc/flourish",
    "description": "A collection of classes for general web development, with a particular focus on compatibility, portability and security",
    "homepage": "http://flourishlib.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Will Bond",
            "role": "Developer",
            "homepage": "http://wbond.net/",
            "email": "will@wbond.net"
        }
